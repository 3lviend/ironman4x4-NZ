<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(subdomain) -%>">
  <span class='title'>
    <%= subdomain.db_name %>

  </span>
  <span class='actions'>
    
    
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_ironman_admin_subdomain_path(subdomain),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.ironman_admin_subdomain_path(subdomain),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :data => {:confirm => t('message', :scope => 'refinery.admin.delete', :title => subdomain.db_name)},
        :method => :delete %>
  </span>
</li>
