<%= render 'refinery/admin/search_header', :url => refinery.ironman_admin_products_path %>
<% if ::Refinery::Ironman::Admin::ProductsController.searchable? %>
    <%= render '/refinery/admin/search', :url => refinery.ironman_admin_products_path %>
<% end %>
<div class='pagination_container'>
  <% if @products.any? %>
    <%= render 'products' %>
  <% else %>
    <p>
      <% unless searching? %>
        <strong>
          <%= t('.no_items_yet') %>
        </strong>
      <% else %>
        <%= t('no_results', :scope => 'refinery.admin.search') %>
      <% end %>
    </p>
  <% end %>
</div>
