<section class="header">
  <aside id="search" class="stockists">
    <%= render 'refinery/admin/search_header', :url => refinery.ironman_admin_stockists_path %>
    <% if ::Refinery::Ironman::Admin::StockistsController.searchable? %>
      <%= render '/refinery/admin/search', :url => refinery.ironman_admin_stockists_path %>
    <% end %>
  </aside>
  <aside id='actions' class='stockists'>
    <%= render 'actions' %>
  </aside>
</section>
<section id='records' class='stockists'>
  <%= render 'records' %>
</section>
<%= render '/refinery/admin/make_sortable', options: {tree: false} if !searching? and ::Refinery::Ironman::Admin::StockistsController.sortable? and ::Refinery::Ironman::Stockist.many? %>
