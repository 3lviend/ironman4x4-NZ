<tr class='record <%= cycle("on", "on-hover") %>' id="<%= dom_id(warehouse) -%>">
  <td class='col title'>
    <%= "#{warehouse.country}, #{warehouse.name}" %>
  </td>
  <td class='col'>
    <%= warehouse.region %>
  </td>
  <td class='col'>
    <%= warehouse.country %>
  </td>
  <td class='col'>
    <%= warehouse.postcode %>
  </td>
  <td class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.ironman_warehouse_path(warehouse),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_ironman_admin_warehouse_path(warehouse),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.ironman_admin_warehouse_path(warehouse),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :data => {:confirm => t('message', :scope => 'refinery.admin.delete', :title => warehouse.name)},
        :method => :delete %>
  </td>
</tr>
