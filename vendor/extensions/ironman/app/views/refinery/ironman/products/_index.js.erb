<% if @this_category.present? %>
  <% cache_key = [@this_category] %>
<% else %>
  <% cache_key = [] %>
<% end %>

<% if @vehicle_filter.present? %>
  <% cache_key = cache_key.push @vehicle_filter %>
<% end %>

<% cache([cache_key_for_products, cache_key, params[:page]]) do %>
$('#page-product-list').append('<%= j render(@products) %>');
<% if @products.next_page %>
  $('.pagination').replaceWith('<%= j will_paginate(@products) %>');
<% else %>
  $('.pagination').remove();
<% end %>
<% end %>
